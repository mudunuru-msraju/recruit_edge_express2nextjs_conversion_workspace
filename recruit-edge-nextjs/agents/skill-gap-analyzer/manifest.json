{
  "id": "skill-gap-analyzer",
  "name": "Skill Gap Analyzer",
  "description": "AI-powered skill analysis with gap identification, learning recommendations, and progress tracking",
  "category": "job-seeker-agents",
  "version": "1.0.0",
  "author": "RecruitEdge",
  "capabilities": [
    "Skill Analysis & Assessment",
    "Gap Identification",
    "Learning Recommendations",
    "Progress Tracking",
    "Resume-Based Analysis",
    "Job Description Matching"
  ],
  "skillCategories": [
    "technical",
    "soft_skill",
    "tool",
    "framework",
    "language",
    "certification"
  ],
  "priorityLevels": ["low", "medium", "high", "critical"],
  "skillLevels": ["none", "beginner", "intermediate", "advanced", "expert"],
  "features": {
    "skillAnalysis": {
      "enabled": true,
      "description": "Analyze current skills against job requirements"
    },
    "gapIdentification": {
      "enabled": true,
      "description": "Identify missing and underdeveloped skills"
    },
    "learningRecommendations": {
      "enabled": true,
      "description": "Provide curated learning resources and paths"
    },
    "progressTracking": {
      "enabled": true,
      "description": "Track skill development progress over time"
    },
    "resumeIntegration": {
      "enabled": true,
      "description": "Extract skills from existing resumes"
    }
  },
  "database": {
    "tables": ["skill_analyses", "skill_gaps"],
    "relationships": {
      "skill_analyses": "belongs to users, resumes",
      "skill_gaps": "belongs to skill_analyses"
    }
  },
  "apiEndpoints": [
    "GET /api/agents/job-seeker-agents/skill-gap-analyzer/analyses",
    "POST /api/agents/job-seeker-agents/skill-gap-analyzer/analyses",
    "GET /api/agents/job-seeker-agents/skill-gap-analyzer/analyses/[id]",
    "PUT /api/agents/job-seeker-agents/skill-gap-analyzer/analyses/[id]",
    "DELETE /api/agents/job-seeker-agents/skill-gap-analyzer/analyses/[id]",
    "GET /api/agents/job-seeker-agents/skill-gap-analyzer/analyses/[id]/gaps",
    "PUT /api/agents/job-seeker-agents/skill-gap-analyzer/analyses/[analysisId]/gaps/[gapId]",
    "POST /api/agents/job-seeker-agents/skill-gap-analyzer/ai/analyze"
  ],
  "dependencies": {
    "zod": "^3.23.8",
    "@tanstack/react-query": "^5.59.0",
    "drizzle-orm": "^0.36.1"
  },
  "ui": {
    "primaryColor": "purple",
    "icon": "chart-bar",
    "landingPage": "/app/agents/job-seeker-agents/skill-gap-analyzer/page.tsx"
  }
}